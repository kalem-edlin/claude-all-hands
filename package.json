{
  "name": "claude-all-hands",
  "version": "1.0.16",
  "description": "CLI for syncing Claude agent configurations to all-hands repository",
  "type": "module",
  "bin": {
    "claude-all-hands": "./bin/cli.js"
  },
  "files": [
    "bin/",
    ".claude/",
    ".allhands-manifest.json"
  ],
  "scripts": {
    "build": "esbuild src/cli.ts --bundle --platform=node --target=node18 --outfile=bin/cli.js --format=esm --banner:js=\"#!/usr/bin/env node\"",
    "dev": "npm run build -- --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "agents",
    "sync",
    "cli"
  ],
  "author": "kalem-edlin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kalem-edlin/claude-all-hands"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/yargs": "^17.0.32",
    "esbuild": "^0.24.0",
    "tsx": "^4.19.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "minimatch": "^10.0.1",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=18"
  }
}
